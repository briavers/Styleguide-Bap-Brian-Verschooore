// -------------------------------------------------------------------
// :: ICON CLASS NAMES
// -------------------------------------------------------------------
// Created with the 'gulp iconfont'-task, the source-template
// of this file can be found in fonts/icons/icon-sources/_template.scss

<% _.each(glyphs, function(glyph) { %>
.<%= className %>-<%= glyph.name %>:before { @extend %icon; content: "\<%= glyph.unicode[0].charCodeAt(0).toString(16).toUpperCase() %>"; }<% }); %>
